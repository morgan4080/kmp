CREATE TABLE IF NOT EXISTS memberDataEntity (
  refId TEXT NOT NULL PRIMARY KEY,
  registrationFees REAL,
  registrationFeeStatus TEXT
);

getMemberData:
SELECT *
FROM memberDataEntity;

insert:
INSERT OR REPLACE INTO memberDataEntity(refId, registrationFees, registrationFeeStatus)
VALUES (?, ?, ?);

removeMemberData:
DELETE FROM memberDataEntity;
